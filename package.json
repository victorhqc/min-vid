    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.5",
    "cross-spawn": "5.1.0",
    "deep-assign": "2.0.0",
    "deploy-txp": "1.0.7",
    "eslint": "3.19.0",
    "eslint-plugin-mozilla": "0.4.2",
