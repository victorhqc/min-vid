    "package": "webpack && ./bin/package-addon.sh",
    "prepackage": "npm run copy:locales && npm run lint",
    "postpackage": "addons-linter addon.xpi -o text",
    "prepush": "npm run package",
    "deploy": "deploy-txp"
  },
